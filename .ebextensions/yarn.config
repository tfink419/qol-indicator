commands:
    01_install_node:
        command: "curl --silent --location https://rpm.nodesource.com/setup_10.x | bash -;"
    02_install_yarn:
        command: "curl --silent --location https://dl.yarnpkg.com/rpm/yarn.repo | tee /etc/yum.repos.d/yarn.repo; yum -y install yarn;"
    03_install_git:
        command: "yum -y install git"